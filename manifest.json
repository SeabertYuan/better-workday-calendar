{
  "name": "Better Workday Calendar",
  "version": "0.9.0",
  "description": "A browser extension that allows you to use the workday calendar term by term.",
  "homepage_url": "https://github.com/SeabertYuan/better-workday-calendar",
  "manifest_version": 3,
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://wd10.myworkday.com/ubc/d/task/2998$28771.htmld"
      ],
      "js": [
        "draw-calendar.js",
        "process-courses.js",
        "index.js"
      ],
      "run_at": "document_idle"
    }
  ]
}
